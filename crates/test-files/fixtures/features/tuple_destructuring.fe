contract Foo {
    pub fn bar() -> u256 {
        let (x, y): (u256, bool) = (42, true)
        return x
    }

    pub fn baz(n: u256, b: bool) -> u256 {
        let (x, y): (u256, bool) = make_tuple(n, b)
        return x
    }

    fn make_tuple(n: u256, b: bool) -> (u256, bool) {
        return (n, b)
    }
}
